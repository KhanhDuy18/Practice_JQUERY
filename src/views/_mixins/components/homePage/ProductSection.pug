                        +BannerProduct("images/productBannerHei.png","images/productBannerLg.png","Gold big hoops", 6800)
                    </div>
                    <ul class="productSection-banner-dots" data-dots-wrapper>
                        <li class="productSection-banne-dot "></li>
                        <li class="productSection-banne-dot dotActive"></li>
                        <li class="productSection-banne-dot"></li>
                        <li class="productSection-banne-dot"></li>
                        <li class="productSection-banne-dot"></li>
                    </ul>
                </div>
            </div>
            <div class="productSection-listProducts">
                <div class="productSection-listProducts-title">
                    <p>Shop The Latest</p>
                    <a href="#">View all</a>
                </div>
                <div class="productSection-listProducts-list">
                    +CardProduct({'productId': 'id001','productName': 'Product1','productImg': 'images/productSmall2.png', 'productPrice': 20000, 'productState': 0.21})
                    +CardProduct({'productId': 'id002','productName': 'Product2','productImg': 'images/productSmall2.png', 'productPrice': 20000, 'productState': 0.21})
