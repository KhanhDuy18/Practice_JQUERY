mixin ProductSection(checkMenu,checkBanner="yes")
    <div class="productSection">
        if checkMenu =="no"
            <div class="productSection-mnenuAndBanner">
                <div class="productSection-banner" data-slider-swiper>
                    <div class="productSection-banner-img" data-slider-wrapper>
                        +BannerProduct("images/productBannerHei.png","images/productBannerLg.png","Gold big hoops", 6800)
                        +BannerProduct("images/productBannerHei.png","images/productBannerLg.png","Gold big hoops", 6800)
                        +BannerProduct("images/productBannerHei.png","images/productBannerLg.png","Gold big hoops", 6800)
